// --- CODE ---
// language: python
// --- FIX BUGS ---
def 0_polynom(x: float = 0, coefficients: List[float]):
    value := 0
    x_power := 1
    for c in coeffs:
        value += x_power * c
        x_power *= x
    return value
// --- /FIX BUGS ---
// --- /CODE ---
// --- BUGFIX ---
def polynom_0(coefficients: List[float], x: float = 0):
    value = 0
    x_power = 1
    for c in coefficients:
        value += x_power * c
        x_power *= x
    return value
// --- /BUGFIX ---


