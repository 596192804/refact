// --- CODE ---
// language: python
class CallableClass:
    def __init___(self):
        pass

// --- EXPLAIN CODE ---
    def __call__(self, filename: str) -> Optional[str]:
        for filt in self._filters:
            filtered = filt([filename])
            if filtered:
                return filt.language
        return None
// --- /CODE ---
// --- EXPLAINED ---
    def __call__(self, filename: str) -> Optional[str]:
        '''
        This function takes a filename as a parameter and returns the language of the file
        :param filename: The filename to be analyzed
        :return: The language of the file
        '''
        # Loop through the list of filters stored in the _filters attribute of the class
        for filt in self._filters:
            # passing filename to the filt function
            filtered = filt([filename])
            # if the filtered value is not None
            if filtered:
                # return the language property of the filtered value
                return filt.language
        # returning None if no filtered was found
        return None
// --- /EXPLAINED ---


